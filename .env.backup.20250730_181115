# AI SEO Blogger 환경변수 설정
# 이 파일을 .env로 복사하고 실제 값으로 수정하세요

# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here

# DeepL API 설정  

# Google Gemini API 설정
GOOGLE_API_KEY=your_google_api_key_here

# 관리자 계정 설정
ADMIN_USERNAME=admin
ADMIN_PASSWORD=0000

# 데이터베이스 설정 (선택사항)
DATABASE_URL=sqlite:///./blog.db

# 디버그 모드 (개발환경에서만 True)
DEBUG=False 